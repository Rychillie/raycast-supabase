{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "supabase-project-manager",
  "title": "Supabase Project Manager",
  "description": "Manage your Supabase project.",
  "icon": "command-icon.png",
  "author": "Rychillie",
  "owner": "pessoal",
  "categories": [
    "Applications",
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "supabaseUrl",
      "title": "Supabase URL",
      "description": "Your Supabase URL",
      "type": "textfield",
      "required": true
    },
    {
      "name": "supabaseKey",
      "title": "Supabase Key",
      "description": "Your Supabase Key",
      "type": "textfield",
      "required": true
    }
  ],
  "commands": [
    {
      "name": "index",
      "title": "List Tables and Details",
      "subtitle": "Show Tables and Details",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.1",
    "@raycast/utils": "^1.4.14"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
